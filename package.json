{
  "name": "fs-app-template",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@mui/material": "^5.12.1",
    "@mui/styled-engine-sc": "^5.12.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@stripe/react-stripe-js": "^1.16.5",
    "@stripe/stripe-js": "^1.48.0",
    "bcrypt": "^5.0.0",
    "bootstrap": "^5.2.3",
    "compression": "^1.7.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^8.7.3",
    "react-bootstrap": "^2.7.2",
    "react-test-renderer": "^18.2.0",
    "sequelize": "^6.20.0",
    "stripe": "^11.13.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^6.2.3",
    "nodemon": "^2.0.16",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^v8.0.1",
    "react-router-dom": "^6.3.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "supertest": "^4.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  }
}
